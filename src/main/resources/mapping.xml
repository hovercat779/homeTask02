<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.CustomersEntity" table="CUSTOMERS" schema="testDB">
        <id name="custNum" column="CUST_NUM"/>
        <property name="company" column="COMPANY"/>
        <property name="creditLimit" column="CREDIT_LIMIT"/>
    </class>
    <class name="entity.OfficesEntity" table="OFFICES" schema="testDB">
        <id name="office" column="OFFICE"/>
        <property name="city" column="CITY"/>
        <property name="region" column="REGION"/>
        <property name="target" column="TARGET"/>
        <property name="sales" column="SALES"/>
    </class>
    <class name="entity.OrdersEntity" table="ORDERS" schema="testDB">
        <id name="orderNum" column="ORDER_NUM"/>
        <property name="orderDate" column="ORDER_DATE"/>
        <property name="qty" column="QTY"/>
        <property name="amount" column="AMOUNT"/>
    </class>
    <class name="entity.ProductsEntity" table="PRODUCTS" schema="testDB">
        <composite-id mapped="true" class="entity.ProductsEntityPK">
            <key-property name="mfrId" column="MFR_ID"/>
            <key-property name="productId" column="PRODUCT_ID"/>
        </composite-id>
        <property name="description" column="DESCRIPTION"/>
        <property name="price" column="PRICE"/>
        <property name="qtyOnHand" column="QTY_ON_HAND"/>
    </class>
    <class name="entity.SalesrepsEntity" table="SALESREPS" schema="testDB">
        <id name="emplNum" column="EMPL_NUM"/>
        <property name="name" column="NAME"/>
        <property name="age" column="AGE"/>
        <property name="title" column="TITLE"/>
        <property name="hireDate" column="HIRE_DATE"/>
        <property name="quota" column="QUOTA"/>
        <property name="sales" column="SALES"/>
    </class>
    <class name="entity.CustomersEntity" table="CUSTOMERS" schema="testDB">
        <id name="custNum">
            <column name="CUST_NUM" sql-type="int(11)"/>
        </id>
        <property name="company">
            <column name="COMPANY" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="custRep">
            <column name="CUST_REP" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="creditLimit">
            <column name="CREDIT_LIMIT" sql-type="decimal(9,2)" precision="9" scale="2" not-null="true"/>
        </property>
    </class>
    <class name="entity.OfficesEntity" table="OFFICES" schema="testDB">
        <id name="office">
            <column name="OFFICE" sql-type="int(11)"/>
        </id>
        <property name="city">
            <column name="CITY" sql-type="varchar(15)" length="15"/>
        </property>
        <property name="region">
            <column name="REGION" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="mgr">
            <column name="MGR" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="target">
            <column name="TARGET" sql-type="decimal(9,2)" precision="9" scale="2" not-null="true"/>
        </property>
        <property name="sales">
            <column name="SALES" sql-type="decimal(9,2)" precision="9" scale="2"/>
        </property>
        <one-to-one name="of" class="entity.SalesrepsEntity"/>
    </class>
    <class name="entity.OrdersEntity" table="ORDERS" schema="testDB">
        <id name="orderNum">
            <column name="ORDER_NUM" sql-type="int(11)"/>
        </id>
        <property name="orderDate">
            <column name="ORDER_DATE" sql-type="date"/>
        </property>
        <property name="cust">
            <column name="CUST" sql-type="int(11)"/>
        </property>
        <property name="rep">
            <column name="REP" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="mfr">
            <column name="MFR" sql-type="char(3)" length="3"/>
        </property>
        <property name="product">
            <column name="PRODUCT" sql-type="char(5)" length="5"/>
        </property>
        <property name="qty">
            <column name="QTY" sql-type="int(11)"/>
        </property>
        <property name="amount">
            <column name="AMOUNT" sql-type="decimal(9,2)" precision="9" scale="2"/>
        </property>
    </class>
    <class name="entity.ProductsEntity" table="PRODUCTS" schema="testDB">
        <composite-id mapped="true" class="entity.ProductsEntityPK">
            <key-property name="mfrId">
                <column name="MFR_ID" sql-type="char(3)" length="3"/>
            </key-property>
            <key-property name="productId">
                <column name="PRODUCT_ID" sql-type="char(5)" length="5"/>
            </key-property>
        </composite-id>
        <property name="description">
            <column name="DESCRIPTION" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="price">
            <column name="PRICE" sql-type="decimal(9,2)" precision="9" scale="2"/>
        </property>
        <property name="qtyOnHand">
            <column name="QTY_ON_HAND" sql-type="int(11)"/>
        </property>
    </class>
    <class name="entity.SalesrepsEntity" table="SALESREPS" schema="testDB">
        <id name="emplNum">
            <column name="EMPL_NUM" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="NAME" sql-type="varchar(15)" length="15"/>
        </property>
        <property name="age">
            <column name="AGE" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="repOffice">
            <column name="REP_OFFICE" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="title">
            <column name="TITLE" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="hireDate">
            <column name="HIRE_DATE" sql-type="date"/>
        </property>
        <property name="manager">
            <column name="MANAGER" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="quota">
            <column name="QUOTA" sql-type="decimal(9,2)" precision="9" scale="2" not-null="true"/>
        </property>
        <property name="sales">
            <column name="SALES" sql-type="decimal(9,2)" precision="9" scale="2"/>
        </property>
        <one-to-one name="sl" class="entity.OfficesEntity"/>
    </class>
    <class name="entity.CustomersEntity" table="CUSTOMERS" schema="testDB">
        <one-to-one name="c" class="entity.OrdersEntity"/>
    </class>
    <class name="entity.OrdersEntity" table="ORDERS" schema="testDB">
        <one-to-one name="o" class="entity.CustomersEntity"/>
    </class>
    <class name="entity.CustomersEntity" table="CUSTOMERS" schema="testDB">
        <id name="custNum">
            <column name="CUST_NUM" sql-type="int(11)"/>
        </id>
        <property name="company">
            <column name="COMPANY" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="custRep">
            <column name="CUST_REP" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="creditLimit">
            <column name="CREDIT_LIMIT" sql-type="decimal(9,2)" precision="9" scale="2" not-null="true"/>
        </property>
    </class>
    <class name="entity.OrdersEntity" table="ORDERS" schema="testDB">
        <id name="orderNum">
            <column name="ORDER_NUM" sql-type="int(11)"/>
        </id>
        <property name="orderDate">
            <column name="ORDER_DATE" sql-type="date"/>
        </property>
        <property name="cust">
            <column name="CUST" sql-type="int(11)"/>
        </property>
        <property name="rep">
            <column name="REP" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="mfr">
            <column name="MFR" sql-type="char(3)" length="3"/>
        </property>
        <property name="product">
            <column name="PRODUCT" sql-type="char(5)" length="5"/>
        </property>
        <property name="qty">
            <column name="QTY" sql-type="int(11)"/>
        </property>
        <property name="amount">
            <column name="AMOUNT" sql-type="decimal(9,2)" precision="9" scale="2"/>
        </property>
    </class>
    <class name="entity.CustomersEntity" table="CUSTOMERS" schema="testDB">
        <one-to-one name="cus" class="entity.OrdersEntity"/>
    </class>
    <class name="entity.OrdersEntity" table="ORDERS" schema="testDB">
        <one-to-one name="ord" class="entity.CustomersEntity"/>
    </class>
</hibernate-mapping>